<div class="recipes-home-body inner-page">
<div class="container">
<div class="row">
<div class="col-md-8 col-lg-9">
<div class="recipe-set">
<div class="listing-buttons">
    <span class="grid current"><i class="fa fa-th-large"></i></span>
    <span class="list "><i class="fa fa-bars"></i></span>
</div>
<h2>Recipes</h2>

<div class="recipe-listing listing-grid">

<% recipes.forEach(el => { %>
    
<div class="listing">
    <div class="image">
        <a href="allRecipes/<%= el._id %>">
            <img src="<%= el.img %>" alt="image"/>
        </a>
    </div>
    <div class="detail">
        <h4><a href="allRecipes/<%= el._id %>"><%= el.title %></a></h4>
        <p>
            <%= el.shortDes %>
        </p>
        <div class="meta-listing">
            <ul class="post-meta">
                <li class="author"><a href="#">John Doe</a></li>
                <li class="calendar"><%= el.createdAt.toLocaleDateString() %></li>
            </ul>          
        </div>
    </div>
</div>

<% }) %>





</div>
